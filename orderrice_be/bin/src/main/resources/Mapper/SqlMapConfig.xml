        <typeAlias alias="Category" type="com.bezkoder.spring.datajpa.model.Category" />
        <typeAlias alias="BillManagementById" type="com.bezkoder.spring.datajpa.model.BillManagementById" />
        <typeAlias alias="ListCategory" type="com.bezkoder.spring.datajpa.request.ListCategory" />
